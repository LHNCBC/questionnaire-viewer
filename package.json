{
  "name": "questionnaire-viewer",
  "version": "0.4.3",
  "description": "A website for rendering a FHIR Questionnaire using the LHC-Forms widget",
  "main": "app.js",
  "scripts": {
    "test": "npm run start-no-open & sleep 1; npm run cy:run; s=$?; kill %1; exit $s",
    "build": "webpack --config webpack.production.js",
    "start": "webpack-dev-server --open --config webpack.development.js",
    "start-no-open": "webpack-dev-server --config webpack.development.js",
    "cy:open": "cypress open --project test",
    "cy:run": "cypress run --project test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lhncbc/questionnaire-viewer.git"
  },
  "keywords": [
    "FHIR"
  ],
  "author": "wangye@nih.gov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.6.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.4",
    "cypress": "^10.10.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.1.0",
    "file-saver": "^2.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-spec-reporter": "^4.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "moment": "^2.24.0",
    "optimize-css-assets-webpack-plugin": "^6.0.0",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "fhirclient": "^2.3.10",
    "js-untar-lhc": "^2.1.1",
    "lforms": "^29.0.3",
    "lforms-updater": "^29.0.1",
    "pako": "^1.0.11",
    "string-to-arraybuffer": "^1.0.2",
    "url-parse": "^1.5.3",
    "whatwg-fetch": "^3.0.0"
  }
}
